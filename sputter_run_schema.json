{
  "definitions": {
    "igsn": {
      "id": "igsn",
      "type": "object",
      "format": "grid-strict",
      "title": "Sample IGSN Creation",
      "options": {
        "grid_columns": 12,
        "titleHidden": true
      },
      "properties": {
        "_text1": {
          "format": "hidden",
