          "enum": ["JH", "TM", "SB"],
          "propertyOrder": 1,
          "options": {
            "titleHidden": true,
            "grid_columns": 3,
            "enum_titles": [
              "Johns Hopkins University",
              "Texas A&M University",
              "University of California, Santa Barbara"
            ]
          }
        },
        "groupJH": {
          "type": "string",
