        },
        "submaterialBM": {
          "type": "string",
          "description": "Material Subtype",
          "enum": ["X"],
          "propertyOrder": 4,
          "options": {
            "titleHidden": true,
            "dependencies": {
              "material": "BM"
            },
            "grid_columns": 3,
            "enum_titles": [
              ""
