            "enum_titles": [
              "alcohols",
              "aldehydes",
              "alkanes",
              "alkenes",
              "alkynes",
              "amines",
              "carboxylic acids",
              "cyclic compounds",
              "cycloalkanes",
              "esters",
              "ketones",
              "nitriles"
            ],
            "grid_columns": 3
