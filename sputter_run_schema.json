        "submaterialOC": {
          "type": "string",
          "description": "Material Subtype",
          "enum": ["A", "B", "C", "D", "E", "F", "G", "H", "I", "J", "K", "L"],
          "propertyOrder": 4,
          "options": {
            "titleHidden": true,
            "dependencies": {
              "material": "OC"
